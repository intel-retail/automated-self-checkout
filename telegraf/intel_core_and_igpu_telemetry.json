[
    {
        "apiVersion": "influxdata.com/v2alpha1",
        "kind": "Dashboard",
        "metadata": {
            "name": "inspiring-brahmagupta-c75001"
        },
        "spec": {
            "charts": [
                {
                    "colors": [
                        {
                            "id": "0",
                            "name": "laser",
                            "type": "min",
                            "hex": "#00C9FF"
                        },
                        {
                            "id": "1",
                            "name": "comet",
                            "type": "max",
                            "hex": "#9394FF",
                            "value": 100
                        }
                    ],
                    "decimalPlaces": 2,
                    "height": 6,
                    "kind": "Gauge",
                    "name": "Intel iGPU Telemetry",
                    "queries": [
                        {
                            "query": "from(bucket: \"telegraf-bucket\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"gpu_usage\")\n  |> filter(fn: (r) => r[\"_field\"] == \"intel_gpu_util\")\n  |> aggregateWindow(every: v.windowPeriod, fn: last, createEmpty: false)\n  |> yield(name: \"last\")"
                        }
                    ],
                    "staticLegend": {},
                    "suffix": "%",
                    "width": 4
                },
                {
                    "colors": [
                        {
                            "id": "0",
                            "name": "laser",
                            "type": "min",
                            "hex": "#00C9FF"
                        },
                        {
                            "id": "1",
                            "name": "comet",
                            "type": "max",
                            "hex": "#9394FF",
                            "value": 100
                        }
                    ],
                    "decimalPlaces": 2,
                    "height": 6,
                    "kind": "Gauge",
                    "name": "Intel CPU Telemetry",
                    "queries": [
                        {
                            "query": "from(bucket: \"telegraf-bucket\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"cpu\")\n  |> filter(fn: (r) => r[\"_field\"] == \"usage_user\")\n  |> filter(fn: (r) => r[\"cpu\"] == \"cpu-total\")\n  |> aggregateWindow(every: 5s, fn: mean, createEmpty: false)\n  |> yield(name: \"mean\")"
                        }
                    ],
                    "staticLegend": {},
                    "suffix": "%",
                    "width": 4,
                    "xPos": 4
                },
                {
                    "height": 6,
                    "kind": "Markdown",
